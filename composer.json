{
    "name": "shopsys/phpstorm-inspect",
    "description": "Package for running PhpStorm's inspections from CLI and displaying results",
    "keywords": ["PhpStorm","inspection","code quality"],
    "license": "MIT",
    "authors": [
        {
            "name": "ShopSys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "ShopSys\\": "src/ShopSys"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/shopsys/coding-standards.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "arvenil/ninja-mutex": "0.4.1",
        "symfony/filesystem": "@stable"
    },
    "require-dev": {
        "shopsys/coding-standards": "3.0.0"
    },
    "bin": [
        "bin/phpstorm-inspect"
    ]
}
